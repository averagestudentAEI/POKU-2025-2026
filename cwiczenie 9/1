#include<stdio.h>
#include<math.h>
#include<stdlib.h>
float parabola(float *q,float *p,float a,float b,float c)
{
	float y,delta;
	delta=(b*b)-(4*a*c);
	*p=(-b)/(2*a);
	*q=(-delta)/(4*a);
		return(delta);
}


main(){
	float a,b,c,p,q,delta;
	printf("daj a,b,c");
	scanf("%f %f %f",&a,&b,&c);
	parabola(&q,&p,a,b,c);
		printf("p i q= %f %f",p,q);
}
